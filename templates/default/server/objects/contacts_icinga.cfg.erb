<% @contacts.each do |contact| %>
define contact {
    <% contact.sort.each do |key, value| %>
        <% next if key == 'password' %>
        <% if value.kind_of?(Array) %>
        <%= key.ljust(30) %> <%= value.join(',') %>
        <% else %>
        <%= key.ljust(30) %> <%= value %>
        <% end %>
    <% end %>
}
<% end %>
